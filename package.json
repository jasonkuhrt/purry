{
  "name": "purry",
  "version": "0.0.0",
  "description": "Symbiotic, high-performance currying and partial application",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha ./test/index.js --reporter spec",
    "test-dev": "./node_modules/.bin/mocha ./test/index.js --reporter spec --watch",
    "test-fuzzy": "PURRY_HISTORY=true node ./test-fuzzy/fuzzy",
    "test-fuzzy-debug": "PURRY_DEBUG=true DEBUG=fuzzy node ./test-fuzzy/fuzzy --report 2",
    "dev": "./node_modules/.bin/mocha ./test/index.js --reporter spec --watch",
    "bench-slow": "node ./bench/index.js | tee bench/bench-results-$(date +%s).txt",
    "bench-fast": "node ./bench/index.js 0.1 | tee bench/bench-results-$(date +%s).txt",
    "bench-sketch": "node ./bench/index.js 0.1"
  },
  "keywords": [
    "functional",
    "function",
    "functions",
    "currying",
    "partial",
    "application",
    "partial-right",
    "api",
    "sugar"
  ],
  "author": "Jason Kuhrt <jasonkuhrt@me.com> (http://jasonkuhrt.com/)",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.15.1",
    "benchmark": "~1.0.0",
    "lodash": "~2.4.1",
    "debug": "~0.7.4",
    "minimist": "0.0.8",
    "assert-plus": "~0.1.5"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jasonkuhrt/purry.git"
  },
  "bugs": {
    "url": "https://github.com/jasonkuhrt/purry/issues"
  },
  "testling": {
    "harness": "mocha-tdd",
    "files": "test/index.js",
    "browsers": [
      "ie/8..latest",
      "firefox/11..latest",
      "chrome/22..latest",
      "safari/4..latest",
      "opera/11..latest",
      "iphone/1..latest",
      "iphone/3..latest",
      "ipad/1..latest",
      "android-browser/1..latest"
    ]
  },
  "homepage": "https://github.com/jasonkuhrt/purry",
  "dependencies": {
    "stack-trace": "0.0.9",
    "args-list": "~0.3.1",
    "ansicolors": "~0.3.2"
  }
}
